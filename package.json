{
    "name": "@nimashoghi/demofile",
    "version": "0.0.6",
    "author": "Nima Shoghi",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/nimashoghi/demofile.git"
    },
    "scripts": {
        "clean": "rm -rf dist/",
        "build": "tsc && mkdir -p ./dist/protobufs/ && tsc --allowJs --declaration false --outDir ./dist/protobufs/ ./src/protobufs/*.js && cp ./src/protobufs/*.d.ts ./dist/protobufs/",
        "rebuild": "yarn clean && yarn build",
        "watch": "tsc -w"
    },
    "main": "dist/demo.js",
    "types": "dist/demo.d.ts",
    "dependencies": {
        "bit-buffer": "0.0.3",
        "bytebuffer": "^5.0.0",
        "lodash": "^4.17.11",
        "long": "^3.0.3",
        "protobufjs": "^6.8.8"
    },
    "devDependencies": {
        "@types/ansi-styles": "^3.2.0",
        "@types/bytebuffer": "^5.0.37",
        "@types/lodash": "^4.14.116",
        "@types/node": "^10.11.3",
        "ansi-styles": "^3.2.1",
        "browserify": "^16.2.3",
        "buffer": "^5.2.1",
        "husky": "^1.1.1",
        "lint-staged": "^7.3.0",
        "minami": "^1.2.3",
        "prettier": "^1.18.2",
        "shx": "^0.3.2",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "tslint-language-service-ts3": "^1.0.0",
        "typedoc": "^0.13.0",
        "typescript": "^3.1.1",
        "typestrict": "^1.0.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,json}": [
            "prettier --write",
            "git add"
        ]
    }
}
